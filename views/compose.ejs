<%- include('./partials/header') %>
<h1 class="composeTitle">Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label class="fs-2" for="blogTitle">Title</label>
        <input type="text" class="form-control" name="blogTitle" autofocus>
        <label for="blogText">Post</label>
        <textarea name="blogText" class="form-control" rows="8" cols="80"></textarea>
    </div>
    <button class="btn btn-primary btn-lg" type="submit">Publish</button>
</form>